package com.api.service.repository;

import java.util.UUID;

import org.springframework.data.jpa.repository.JpaRepository;

import com.api.service.entity.composite.PermissionKey;
import com.github.andrewoma.dexx.collection.List;

public interface PermissionKeyRepository extends JpaRepository<PermissionKey, UUID> {
	List<PermissionKey> findByRoleId(UUID roleId);
}